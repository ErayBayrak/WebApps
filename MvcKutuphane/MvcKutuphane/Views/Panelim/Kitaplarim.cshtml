@using MvcKutuphane.Models.Entity
@model List<TblHareket>
@{
    ViewBag.Title = "Kitaplarim";
    Layout = "~/Views/Shared/_LayoutPanelim.cshtml";
}

<br />
<h2>Kitap Geçmişim</h2>
<br />
<table class="table table-bordered">
    <tr>
        <th>KİTAP ADI</th>
        <th>ALIŞ TARİHİ</th>
        <th>ÜYE İADE TARİHİ</th>
    </tr>
    @foreach (var x in Model)
        {
            <tr>
                <td>@x.TblKitap.AD</td>
                <td>@Convert.ToDateTime(x.ALISTARIH).ToString("dd/MM/yyyy") </td>
                <td>@Convert.ToDateTime(x.UYEGETIRTARIH).ToString("dd/MM/yyyy")</td>
            </tr>
        }
</table>